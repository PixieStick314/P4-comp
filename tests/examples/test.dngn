Map(10, 10) BossRoom {
    layer terrain
    procedure {
        for i in 0..10 {
            let row = []
            for j in 0..10 {
                if (i == 0 or i == 9) {
                    row += "#"
                }
                elif (j == 0 or j == 9) {
                    row += "#"
                }
                else {
                    row += "."
                }
            }
        terrain[i] = row
        }
    }
    }
