output TileMap BossRoom {
     procedure {
         map = []
         for i in 0..10 {
            let row = []
            for j in 0..10 {
                if (i == 0 or i == 9) {
                    row += "#"
                }
                elif (j == 0 or j == 9) {
                    row += "#"
                }
                else {
                    row += "."
                }
            }
            map += row
         }
     }
     output map
}
